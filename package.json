{
  "name": "lazysex",
  "version": "0.0.0",
  "description": "Fucking great functions for working with sequences. FP-style. Yo.",
  "main": "./lib/index.js",
  "devDependencies": {
    "LiveScript": "~1.1.1",
    "es5-shim": "~2.0.5",
    "brofist-tap": "~0.1.0",
    "brofist": "~0.2.2",
    "chai": "~1.6.0"
  },
  "scripts": {
    "cc": "./node_modules/.bin/lsc -o lib -c src/*.ls",
    "test-cc": "./node_modules/.bin/lsc -o lib-test -c test/*.ls && ./node_modules/.bin/lsc -o lib-test/specs -c test/specs/*.ls",
    "pretest": "npm run-script cc && npm run-script test-cc",
    "test": "brofist-cli ./lib-test/specs/stream.js",
    "prepublish": "npm run-script cc"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/killdream/sex.git"
  },
  "author": "Quildreen \"Sorella\" Motta <quildreen@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "calliope": {
    "output": "docs/api",
    "copyright": "Â© 2013 Quildreen \"Sorella\" Motta",
    "packages": [
      {
        "files": [
          "src/*.ls"
        ]
      }
    ]
  },
  "testling": {
    "files": "test/browser/suite.js",
    "preprocess": "npm run-script pretest-browser",
    "browsers": {
      "ie": [
        8,
        9,
        10
      ],
      "firefox": [
        15,
        "nightly"
      ],
      "chrome": [
        21,
        22,
        "canary"
      ],
      "opera": [
        10,
        12,
        "next"
      ],
      "safari": [
        5.1
      ]
    }
  },
  "dependencies": {
    "boo": "~1.2.4"
  }
}
